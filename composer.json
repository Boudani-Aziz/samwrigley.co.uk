{
    "name": "samwrigley/samwrigley.co.uk",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^1.0",
        "graham-campbell/markdown": "^12.0",
        "jtant/laravel-env-sync": "^4.0",
        "laravel/framework": "^7.0",
        "laravel/telescope": "^3.2",
        "laravel/tinker": "^2.0",
        "nothingworks/blade-svg": "^0.3",
        "spatie/laravel-csp": "^2.5",
        "spatie/laravel-feed": "^2.6",
        "spatie/laravel-view-models": "^1.3",
        "spatie/schema-org": "^2.12"
    },
    "require-dev": {
        "facade/ignition": "^2.0",
        "fzaninotto/faker": "^1.9",
        "laravel/dusk": "^5.11",
        "mockery/mockery": "^1.3",
        "nunomaduro/collision": "^4.1",
        "nunomaduro/larastan": "^0.5",
        "nunomaduro/phpinsights": "^1.14",
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/css-selector": "^5.0",
        "timacdonald/log-fake": "^1.6"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
